CREATE TABLE `db`.`job` (
  `wantedAuthNo` VARCHAR(45) NOT NULL,
  `regDt` DATE NOT NULL,
  `company` VARCHAR(100) NOT NULL,
  `sal` VARCHAR(45) NOT NULL,
  `minSal` INT NOT NULL,
  `maxSal` INT NULL,
  `avgSal` INT NULL,
  `region` VARCHAR(45) NOT NULL,
  `wantedInfoUrl` VARCHAR(300) NOT NULL,
  `wantedTitle` VARCHAR(200) NOT NULL,
  `salTpCd` VARCHAR(45) NOT NULL,
  `jobCont` VARCHAR(2000) NULL,
  `category` VARCHAR(200) NULL,
  `techStack` VARCHAR(200) NULL,
  `enterTpCd` VARCHAR(45) NOT NULL,
  `minEdubgIcd` VARCHAR(45) NOT NULL,
  `pfCond` VARCHAR(500) NULL,
  `empTpNm` VARCHAR(100) NOT NULL,
  `workdayWorkhrCont` VARCHAR(1000) NOT NULL,
  `receiptCloseDt` DATE NOT NULL,
  `logo` VARCHAR(500) NULL,
  `likeNo` INT NULL DEFAULT 0,
  `view` INT NULL DEFAULT 0,
  `indTpCdNm` VARCHAR(200) NULL,
  `reperNm` VARCHAR(45) NULL,
  `corpAddr` VARCHAR(300) NULL,
  `homePg` VARCHAR(500) NULL,
  PRIMARY KEY (`wantedAuthNo`));
